{
  "name": "JD Login",
  "version": "1.0",
  "description": "A Chrome extension that disables qrcode login of jd.com.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://passport.jd.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
